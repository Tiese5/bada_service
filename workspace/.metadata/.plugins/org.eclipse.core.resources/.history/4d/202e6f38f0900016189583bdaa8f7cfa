package study.jsp.mysite.test;

public class NetxTest {

	public static void main(String[] args) {
		BbsDocument bbsDocument = new BbsDocument();
		bbsDocument.setCategory("free");
		bbsDocument.setId(4);
		
		Logger loggel = LogManager.getFormatterLogger();
		SqlSession sqlSession = MyBatisConnectionFactory.getSqlSession();
		
		BbsDocumentService bbsDocumentService = new BbsDocumentServiceImpl(sqlSession,loggel);
		
		try {
			bbsDocumentService.selectPrevDocument(bbsDocument);
		} catch (Exception e) {
			System.out.println(e.getLocalizedMessage());
			e.printStackTrace();
		}
		System.out.println(bbsDocument.getId()+"번째 게시물");
				
	}
}
